<?xml version="1.0" encoding="utf-8"?>

<objects xmlns="http://www.springframework.net">

  <object id="HoldkassenDbContext" type="HoldkassenAPI.DAL.HoldkassenDbContext, HoldkassenAPI"
          singleton="false" />

  <object id="TeamReadRepo"
          type="HoldkassenAPI.Repositories.Read.TeamReadRepo, HoldkassenAPI" singleton="false">
    <constructor-arg ref="HoldkassenDbContext" />
  </object>

  <object id="TeamWriteRepo"
          type="HoldkassenAPI.Repositories.Write.TeamWriteRepo, HoldkassenAPI" singleton="false">
    <constructor-arg ref="HoldkassenDbContext" />
  </object>

  <object id="TeamServices"
          type="HoldkassenAPI.Services.Team.TeamServices, HoldkassenAPI" singleton="false">
    <constructor-arg ref="TeamReadRepo" />
    <constructor-arg ref="TeamWriteRepo" />
  </object>

  <object id="TeamController"
          type="HoldkassenAPI.Controllers.Team.TeamController, HoldkassenAPI" singleton="false">
    <constructor-arg ref="TeamServices" />
  </object>

</objects>
